{
  "expo": {
    "name": "Sage",
    "slug": "sage-financial-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/sage-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/sage-icon.png",
        "backgroundColor": "#f97316"
      },
      "package": "com.pedrodellolio.sagefinancialmobile",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/sage-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#f97316"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ee852581-9709-42a9-8cf9-dcdf431e3c03"
      }
    },
    "owner": "pedrodellolio"
  }
}